{
  "name": "ai-buyer",
  "private": true,
  "version": "1.0.0",
  "description": "AI-powered Facebook advertising automation platform",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "lint": "cd frontend && npm run lint",
    "preview": "cd frontend && npm run preview",
    "install:frontend": "cd frontend && npm install",
    "backend:dev": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "start": "concurrently \"npm run backend:dev\" \"npm run dev\"",
    "setup": "npm run install:frontend && echo 'Please activate Python venv and install backend dependencies'"
  },
  "keywords": ["ai", "facebook", "advertising", "automation", "marketing"],
  "author": "AI-Buyer Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
